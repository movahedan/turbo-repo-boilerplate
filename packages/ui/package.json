{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./*": "./src/*/index.ts",
    "./get-messages": "./src/get-messages.ts"
  },
  "scripts": {
    "type-check": "tsc --project ./tsconfig.json",
    "lint": "eslint . --max-warnings 0",
    "lint-locale": "npm exec --yes -- @inlang/cli lint --project project.inlang",
    "test": "jest --color",
    "build": "tsup --onSuccess 'tsc --project tsconfig.prod.json'",
    "storybook": "storybook dev -p 6200 --quiet",
    "storybook-build": "storybook build -o dist/storybook",
    "generate:component": "turbo gen react-component",
    "update-dependencies": "npm exec npm-check-updates -- -u"
  },
  "dependencies": {
    "@mui/base": "^5.0.0-beta.40",
    "@repo/utilities": "*",
    "@tailwindcss/forms": "0.5.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.3"
  },
  "devDependencies": {
    "@repo/config-eslint": "*",
    "@repo/config-jest": "*",
    "@repo/config-storybook": "*",
    "@repo/config-tailwind": "*",
    "@repo/config-typescript": "*",
    "@repo/utilities-test": "*",
    "@turbo/gen": "1.13.3"
  }
}
