{
  "name": "my-clicky-game",
  "private": true,
  "scripts": {
    "build": "turbo build --no-cache --",
    "dev": "turbo dev --no-cache --",
    "lint": "turbo lint --no-cache --",
    "test": "turbo test --no-cache --",
    "type-check": "turbo type-check --no-cache --",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.21",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
